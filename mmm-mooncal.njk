{% if config.aspect == "landscape" %}
<div class="container" style="grid-template-columns: repeat(7, 100px); grid-template-rows: 30px repeat({{ config.weeks }}, 110px)">
	{% for i in range(0, 7) -%}
		<div class="wkDayBox small">{{ weekDays[i] }}</div>
  {%- endfor %}
	{% for a in range(0, config.weeks ) -%}
		{% for b in range(1, 8) -%}
			<div class="moonBox date-{{ a * 7 + b }} small">
        <div class="dateHeader">{{ dates[a-1+b*7].day }}</div>
				<div class="symbols">
          <span class="{{ dates[a-1+b*7].symbol1 }}"></span>
          <span class="{{ dates[a-1+b*7].symbol2 }}"></span>
        </div>
				<div class="moon {{ dates[a-1+b*7].moon }}"></div>
			</div>
		{%- endfor %}
	{%- endfor %}
</div>

{% elif config.aspect == "vertical" %}
<div class="container" style="grid-template-columns: 30px repeat({{ config.weeks }}, 100px); grid-template-rows: repeat(7, 110px)">
  {% for a in range(1, 8) -%}
    <div class="wkDayBox small">{{ weekDays[a-1] }}</div>
    {% for b in range(0, config.weeks) -%}
			<div class="moonBox date-{{ a + b * 7 }}">
				<div class="dateHeader">{{ dates[a-1+b*7].day }}</div>
				<div class="symbols">
          <span class="{{ dates[a-1+b*7].symbol1 }}"></span>
          <span class="{{ dates[a-1+b*7].symbol2 }}"></span>
        </div>
				<div class="moon {{ dates[a-1+b*7].moon }}"></div>
			</div>
		{%- endfor %}
	{%- endfor %}
</div>

{% else %}

{% endif %}
